<!DOCTYPE html>
<html lang="en">

{{> head}}

<body>

    {{> navbar}}
    <div class="container">


        

      <div class="mt-sm-5">
            <table class="table">
                <thead style="background-color: black;color: white;">
                    <tr>
                        <th>Cedula</th>
                        <th>Nombre</th>
                        <th>Apellido</th>
                        <th>Email</th>
                    </tr>
                </thead>
                <tbody>
                      {{# each usuarios}}
                    <tr>
                       
                        <th scope="row">{{this.cedula}}</th>
                        <td>{{this.nombre}}</td>
                        <td>{{this.appellido}}</td>
                        <td>{{this.email}}</td>
                    </tr>
                    
         {{/each}} 
                </tbody>
            </table>

        </div>


    </div>
    {{> footer}}
</body>
<!-- Footer -->


</html>